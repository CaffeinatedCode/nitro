{
  "name": "nitro",
  "version": "3.0.0-alpha",
  "description": "Not just another todo list app",
  "scripts": {
    "build": "webpack --progress --colors -p",
    "watch": "webpack --progress --colors --watch",
    "build-css": "node-sass source/scss -o dist/generated",
    "watch-css": "npm run build-css && node-sass source/scss -o dist/generated --watch",
    "test": "cross-env NODE_ENV=test mocha tests.js --compilers js:babel-core/register",
    "coverage": "cross-env NODE_ENV=test nyc mocha tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nitrotasks/nitro.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nitrotasks/nitro/issues"
  },
  "homepage": "http://nitrotasks.com",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-preset-env": "^1.5.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.0",
    "eslint": "^4.0.0",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.15.3",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "nyc": "^11.0.1",
    "webpack": "^2.2.0"
  },
  "dependencies": {
    "preact": "^8.1.0",
    "preact-router": "^2.5.1"
  }
}
