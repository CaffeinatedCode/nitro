{
  "name": "nitrotasks",
  "version": "3.0.0-alpha.1.5",
  "description": "Not just another todo list app",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --colors -p --display-optimization-bailout",
    "report": "cross-env NODE_ENV=report webpack --progress --colors -p --display-optimization-bailout",
    "watch": "webpack --progress --colors --watch",
    "clean": "rm -f dist/generated/*",
    "test": "cross-env NODE_ENV=test mocha tests.js --compilers js:babel-core/register",
    "coverage": "cross-env NODE_ENV=test nyc mocha tests.js",
    "prepublishOnly": "npm run clean && npm run build && npm run build-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nitrotasks/nitro.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/nitrotasks/nitro/issues"
  },
  "homepage": "http://nitrotasks.com",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.3.0",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "nyc": "^11.1.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "preact": "^8.2.1",
    "preact-router": "^2.5.5",
    "webpack-bundle-analyzer": "^2.9.0"
  }
}
