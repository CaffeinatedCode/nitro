@media print
	header.group, aside.sidebar, .main .tasks-container input.new-task
		display: none !important

	section.main
		position: static
		padding-right: 0

		.title
			height: 50px;

		.tasks-container ul.tasks li.task

			&:last-child
				margin-bottom: 0

		&::after
			content: "Created with Nitro Tasks"
			font-size: 11px
			color: #666
			margin: 1em 0
			display: block
			text-align: right
