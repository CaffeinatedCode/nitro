<li class="task <% if @completed: %>completed<% end %> p<%= @priority %>">
  <div class="priority"></div>
  <div class="checkbox"></div>
  <div class="name" contenteditable="true"><%- @name or "<i>No name</i>" %></div>
  <div class="delete"></div>
  <div class="notes" contenteditable="true"><%= @notes or "Notes:" %></div>
</li>
