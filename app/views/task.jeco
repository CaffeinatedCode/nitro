<li class="<% if @completed: %>completed<% end %> p<%= @priority %>">
  <div class="toggle"></div>
  <div class="show">
    <span class="name"><%- @name or "<i>No name</i>" %></span>
    <button class="delete">Delete</button>
  </div>
  <div class="edit">
    <input type="text" class="name">
  </div>
</li>
